<h1>New Entry</h1>
<form method="POST" action="/entries/new" id="newentryform">
	<div class="form-group">
    	<label for="concept">Concept</label>
    	<input type="text" class="form-control" id="concept" name="concept" placeholder="Concpet"/>
  </div>
  <div class="form-group">
    	<label for="value">Ammount</label>
    	<input type="number" class="form-control" id="value" name="value" value="0"/>
  </div>
  <div class="form-group">
    	<label for="date">Date</label>
    	<input type="date" class="form-control" id="date" name="date">
  </div>
  <input type="hidden" name="flags" id="flags" value="0"/>
  <input type="hidden" name="account" id="account" value="{{account}}"/>
  <input type="submit" value="Create"/>
</form>